 * || "Hexadecimal:\t%x %x %X %#x\n" ||  5, 10, 10, 6 || "Hexadecimal: 5 a A 0x6" ||
 * || "Octal:\t%o %#o %#o\n" || 10, 10, 4 || "Octal: 12 012 04" ||
