 * || g || Floating point, shortest representation (%e or %f) || 512.23 ||
 * || G || Floating point, shortest representation (%E or %F) || 512.23 ||
 * || n || Nothing || ||
 * || o || Octal || 320 ||
 * || p || Pointer address || b2010020 ||
 * || s || String || my str ||
 * || u || Decimal integer || 93535 ||
 * || x || Integer, hexadecimal, lowercase || 3ab ||
 * || X || Integer, hexadecimal, uppercase || 3AB ||
